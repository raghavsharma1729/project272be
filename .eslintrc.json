{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-underscore-dangle": "off",
        "object-curly-newline": "off",
        "implicit-arrow-linebreak": "off"
    }
}
